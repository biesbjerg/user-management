{
    "name": "biesbjerg/user-management",
    "description": "User management",
    "require": {
        "php": ">=7.4",
        "cakephp/database": "^4.1",
        "odan/session": "^5.0",
        "php-di/php-di": "^6.2",
        "robmorgan/phinx": "^0.12.4",
        "slim/http": "^1.0",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.0",
        "vlucas/phpdotenv": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "cs:check": "phpcs",
        "cs:fix": "phpcbf"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpro/grumphp": "^0.20.0"
    },
    "config": {
        "sort-packages": true
    }
}
