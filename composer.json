{
    "name": "biesbjerg/user-management",
    "description": "User management",
    "require": {
        "php": ">=7.4",
        "cakephp/database": "^4.1",
        "odan/session": "^5.0",
        "php-di/php-di": "^6.2",
        "robmorgan/phinx": "^0.12.4",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.0",
        "vlucas/phpdotenv": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "cs:check": "phpcs",
        "cs:fix": "phpcbf",
        "phpstan": "phpstan analyze",
        "check": [
            "@cs:check",
            "@phpstan"
        ]
    },
    "require-dev": {
        "phpro/grumphp": "^0.20.0",
        "phpstan/phpstan": "^0.12.40",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "sort-packages": true
    }
}
